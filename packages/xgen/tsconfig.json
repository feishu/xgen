{
	"extends": "./.umi/tsconfig.json",
	"compilerOptions": {
		"jsx": "react",
		"experimentalDecorators": true,
		"paths": {
			"@/*": ["*"],
			"@@/*": [".umi/*"],
			"#/*": ["types/*"],
